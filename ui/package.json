  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "appver": "find src -type f -print0 | LC_COLLATE=C sort -z | xargs -0 sha1sum | sha1sum | cut -c1-7",
    "start": "REACT_APP_VERSION=$(yarn --silent appver) react-scripts start",
    "build": "REACT_APP_VERSION=$(yarn --silent appver) react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/hls.js": "^0.13.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "prettier": "2.2.1",
    "source-map-explorer": "^2.5.2"
  },
  "proxy": "http://localhost:80"
}
