  "private": true,
  "dependencies": {
    "bootstrap": "^4.6.2",
    "hls.js": "^1.4.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.6",
    "react-bootstrap-icons": "^1.10.3",
    "react-dnd": "^15.1.2",
    "react-dnd-html5-backend": "^15.1.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.5",
    "react-full-screen": "^1.1.1",
    "react-hls-player": "^3.0.7",
    "react-router-dom": "^5.3.4",
    "react-scripts": "^5.0.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "appver": "find src -type f -print0 | LC_COLLATE=C sort -z | xargs -0 sha1sum | sha1sum | cut -c1-7",
    "start": "REACT_APP_VERSION=dev react-scripts start",
    "build": "REACT_APP_VERSION=$(yarn --silent appver) react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-router-dom": "^5.3.3",
    "prettier": "2.8.8",
    "source-map-explorer": "^2.5.3"
  },
  "proxy": "http://localhost:80"
}
