    "bootstrap": "^4.6.0",
    "lodash": "^4.17.21",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-bootstrap-icons": "^1.2.3",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
